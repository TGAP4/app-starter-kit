{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"nodemon -w src/server -x tsx src/server/server.ts\" \"bun run codegen\"",
    "start": "NODE_ENV=production bun src/server/server.ts",
    "build": "vite build",
    "codegen": "nodemon -w src/server -e ts,js -x graphql-codegen",
    "migrate": "prisma migrate dev",
    "studio": "prisma studio",
    "prepare": "husky"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@graphql-yoga/plugin-disable-introspection": "^2.6.0",
    "@pothos/core": "^3.41.2",
    "@pothos/plugin-prisma": "^3.65.3",
    "@prisma/client": "^5.16.1",
    "@tanstack/react-query": "^5.50.1",
    "concurrently": "^8.2.2",
    "express": "^4.18.2",
    "framer-motion": "^11.2.13",
    "gql.tada": "^1.8.1",
    "graphql": "^16.9.0",
    "graphql-request": "^7.1.0",
    "graphql-yoga": "^5.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.24.1",
    "styled-components": "^6.1.11",
    "tsx": "^4.3.0",
    "typescript": "^5.5.3",
    "vite-express": "*"
  },
  "devDependencies": {
    "@eslint/js": "^9.7.0",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/client-preset": "^4.3.2",
    "@graphql-eslint/eslint-plugin": "^3.20.1",
    "@parcel/watcher": "^2.4.1",
    "@tanstack/eslint-plugin-query": "^5.51.1",
    "@types/express": "^4.17.21",
    "@types/node": "^20.9.3",
    "@types/react": "^18.0.38",
    "@types/react-dom": "^18.2.16",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "8.x",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.4",
    "globals": "^15.8.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "nodemon": "^3.0.1",
    "prettier": "3.3.3",
    "prettier-plugin-prisma": "^5.0.0",
    "prisma": "^5.16.1",
    "ts-node": "^10.9.2",
    "typescript-eslint": "^7.16.0",
    "vite": "^5.0.2",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "lint-staged": {
    "*.{ts,tsx,js}": "bun run eslint ."
  }
}
