{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"nodemon -w src/server -x tsx src/server/server.ts\" \"bun run codegen\"",
    "start": "NODE_ENV=production bun src/server/server.ts",
    "build": "vite build",
    "codegen": "nodemon -w src/server -e ts,js -x graphql-codegen",
    "migrate": "prisma migrate dev",
    "studio": "prisma studio"
  },
  "dependencies": {
    "@graphql-yoga/plugin-disable-introspection": "^2.6.0",
    "@pothos/core": "^3.41.2",
    "@pothos/plugin-prisma": "^3.65.3",
    "@prisma/client": "^5.16.1",
    "@tanstack/react-query": "^5.50.1",
    "concurrently": "^8.2.2",
    "express": "^4.18.2",
    "gql.tada": "^1.8.1",
    "graphql": "^16.9.0",
    "graphql-request": "^7.1.0",
    "graphql-yoga": "^5.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.24.1",
    "tsx": "^4.3.0",
    "typescript": "^5.5.3",
    "vite-express": "*"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/client-preset": "^4.3.2",
    "@parcel/watcher": "^2.4.1",
    "@tanstack/eslint-plugin-query": "^5.50.1",
    "@types/express": "^4.17.21",
    "@types/node": "^20.9.3",
    "@types/react": "^18.0.38",
    "@types/react-dom": "^18.2.16",
    "@vitejs/plugin-react": "^4.2.0",
    "nodemon": "^3.0.1",
    "prisma": "^5.16.1",
    "ts-node": "^10.9.2",
    "vite": "^5.0.2",
    "vite-tsconfig-paths": "^4.3.2"
  }
}